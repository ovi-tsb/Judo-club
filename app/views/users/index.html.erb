
<div class="user-front-page">
  <div class="container-full-width">
    <div class="main-container">
      <h2>User Page</h2>
      <h3>Hello <%= current_user.email %></h3>
      <div>
        You have <%= current_user.members.count %> active members.
        <table class="table table-striped table-hover">
          <thead>
            <tr class="sortable-table">
              <!-- <th>
                Id
              </th> -->
              <th>
                First Name
              </th>
              <th>
                Last Name
              </th>
              <th>
                Birth Year
              </th>
              <th>
                Active
              </th>
            </tr>
          </thead>
        
          <tbody>
            <%= render 'member' %> 
          </tbody> 
        </table>  
        <!-- Button trigger modal -->
        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter">
          Add/Remove members
        </button>

        <!-- Modal -->
